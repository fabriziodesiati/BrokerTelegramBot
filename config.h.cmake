/* System name */
#cmakedefine APP_SYSTEM_NAME "${APP_SYSTEM_NAME}"

/* Software release */
#cmakedefine APP_SOFTWARE_RELEASE "${APP_SOFTWARE_RELEASE}"

/* Plain software release */
#cmakedefine APP_PLAIN_RELEASE "${APP_PLAIN_RELEASE}"

/* Tags */
#cmakedefine APP_TAGS "${APP_TAGS}"

/* Enable heavy logging */
