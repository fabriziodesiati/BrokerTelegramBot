#cmakedefine APP_VMICRO ${APP_VMICRO}
#cmakedefine APP_VPATCH ${APP_VPATCH}

/* Version string */
#cmakedefine SPACEKIT_APP_VERSION "${SPACEKIT_APP_VERSION}"
